{
  "name": "note-taker",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=11.14.0"
  },
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "start:dev": "nodemon server.js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "coveragePathIgnorePatterns": [
